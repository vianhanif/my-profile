    "express": "^4.16.3",
    "express-mailer": "^0.3.1",
    "font-awesome": "^4.7.0",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "include-media": "^1.4.9",
    "lodash": "^4.17.10",
    "module-alias": "^2.0.6",
    "react": "^16.3.2",
